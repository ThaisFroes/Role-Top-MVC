@model Role_MVC.ViewModel.HistoricoViewModel
@{
    ViewData["Title"] = "Pedidos - Role_MVC";
    ViewData["H2"] = "Hist√≥rico de Pedidos";
}

<main>
    <h2>@ViewData["H2"]</h2>
    <table>
        <thead>
            <tr>
                <th>Data</th>
                <th>Produto Eventos</th>
            </tr>
        </thead>
        <tbody>
            @foreach( var pedido in Model.Pedidos ) {
                <tr>
                    <td>@pedido.DataDoPedido</td>
                    <td>@pedido.ProdutoEvento.Nome</td>
                </tr>
            }
        </tbody>
    </table>

</main>